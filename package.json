{
  "name": "domestique",
  "version": "1.4.0",
  "description": "A modular DOM helper library.",
  "repository": "https://github.com/jsor/domestique.git",
  "author": {
    "name": "Jan Sorgalla",
    "url": "https://sorgalla.com",
    "email": "jsorgalla@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "dom",
    "helper",
    "utility",
    "dimension",
    "element",
    "event",
    "query",
    "selector",
    "on",
    "off",
    "ready"
  ],
  "files": [
    "src",
    "index.js"
  ],
  "xo": {
    "space": 4,
    "rules": {
      "max-params": [
        "warn",
        {
          "max": 5
        }
      ]
    },
    "ignores": [
      "karma.conf.js"
    ],
    "globals": [
      "after",
      "afterEach",
      "assert",
      "before",
      "beforeEach",
      "describe",
      "it"
    ],
    "envs": [
      "node",
      "browser"
    ]
  },
  "scripts": {
    "lint": "xo",
    "build:test": "webpack --config test/webpack.config.js test/index.js --output test/build.js",
    "dev": "karma start",
    "test": "npm run lint && karma start --single-run",
    "test:browserstack": "npm run build:test && browserstack-runner"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "browserstack-runner": "^0.8.0",
    "chai": "^4.1.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.1.0",
    "xo": "^0.22.0"
  }
}
